cd /

; move asm and exe to subdrawers

; sasc
; simple_saga0
IF EXISTS simple_saga0_sasc 
    copy simple_saga0_sasc bin/
    delete simple_saga0_sasc
ENDIF
IF EXISTS simple_saga0_sasc.s 
    copy simple_saga0_sasc.s asm/
    delete simple_saga0_sasc.s
ENDIF
