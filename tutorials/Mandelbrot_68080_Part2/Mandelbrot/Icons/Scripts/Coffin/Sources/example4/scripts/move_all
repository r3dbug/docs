cd /

; move asm and exe to subdrawers

; sasc
; parmandel0
IF EXISTS parmandel0_sasc 
    copy parmandel0_sasc bin/
    delete parmandel0_sasc
ENDIF
IF EXISTS parmandel0_sasc.s 
    copy parmandel0_sasc.s asm/
    delete parmandel0_sasc.s
ENDIF
